.datamanager
  height calc(100vh - var(--header-height))

  // hack to change background of selected annotation in LSF inside DM
  #label-studio-dm [class*=Annotations_annotation_selected]
    background: #e3f1ff

// 数据清洗
.cleandata__header
  display flex
  justify-content space-between
  line-height 36px
.cleandata__wrapper
  overflow hidden
.cleandata__title
  font-size 16px
  font-weight bold
.cleandata__content
  margin 16px -24px -40px
